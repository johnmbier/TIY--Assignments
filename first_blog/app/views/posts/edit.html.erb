<h1>Edit <%= @post.title %></h1>


<%= form_for @post do |f| %>
<br />
<%= f.label :title %>
<%= f.text_field :title%>
<%= f.label :author%>
<%= f.text_field :author%><br>
 <br />
 <%= f.label :content %><br>
 <%= f.text_area :content, :size => "70x20" %>
 <br>
<%= f.submit 'Use the Force Again' %>
<% end %>


